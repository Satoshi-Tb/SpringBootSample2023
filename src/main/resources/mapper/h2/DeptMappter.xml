<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- Mapperとxmlのマッピング -->
<mapper namespace="com.example.repositry.DeptMapper">
  <select id="findAll" resultType="com.example.domain.user.model.Department">
    select
        department_id
        , department_name
    from
      m_department
    order by
      department_id
  </select>
</mapper>